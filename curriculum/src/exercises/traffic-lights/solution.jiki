// Traffic Light

set radius to 10
set center_x to 50
set top_y to 25
set middle_y to 50
set bottom_y to 75

// Background
rectangle(0, 0, 100, 100, "#333333")

// Traffic light housing
rectangle(30, 10, 40, 80, "#222222")

// Red light (top)
circle(center_x, top_y, radius, "#FF0000")

// Yellow light (middle)
circle(center_x, middle_y, radius, "#FFFF00")

// Green light (bottom)
circle(center_x, bottom_y, radius, "#00FF00")

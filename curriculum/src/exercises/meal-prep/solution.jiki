function in_list with items, target do
  for each item in items do
    if item == target do
      return true
    end
  end
  return false
end

function shopping_list with fridge, recipe do
  set list to []
  for each ingredient in recipe do
    if not in_list(fridge, ingredient) do
      change list to push(list, ingredient)
    end
  end
  return list
end